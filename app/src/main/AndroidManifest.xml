<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.ynagarjuna1995.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="io.github.ynagarjuna1995.tourguide.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.ynagarjuna1995.tourguide.PlaceDetailActivity"
            android:parentActivityName="io.github.ynagarjuna1995.tourguide.PlaceActivity">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.github.ynagarjuna1995.tourguide.PlaceActivity"
            android:label="@string/title_text"
            android:parentActivityName="io.github.ynagarjuna1995.tourguide.MainActivity">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>